%populate {
    object Ethernet.Link {
        instance add (Alias = "LO", Name = "lo") {
            parameter Enable = "true";
        }
        instance add ("ETH1", Name = "eth1") {
            parameter Enable = "true";
            parameter LowerLayers = "Device.Ethernet.Interface.1.";
        }
        instance add ("LAN", Name = "br-lan") {
            parameter Enable = "true";
            parameter LowerLayers = "Device.Bridging.Bridge.1.Port.1.";
        }
    }
}
