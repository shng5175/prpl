%populate {
    object 'Time.${prefix_}TimeServer' {
        parameter Enable = true;
        object 'Intf' {
            instance add(Alias="cpe-br-lan") {
                parameter Interface = "br-lan";
                parameter Enable = true;
            }
            instance add(Alias="cpe-br-guest") {
                parameter Interface = "br-guest";
                parameter Enable = true;
            }
        }
    }
}
