
config wifi-device 'wlan0'
        option type 'mac80211'
        option channel '11'
        option hwmode '11g'
        option path 'platform/soc/a000000.wifi'
        option htmode 'HT20'
        option disabled '0'

config wifi-iface 'default_wlan0'
        option device 'wlan0'
        option mode 'ap'
        option ssid 'PrplOs'
        option encryption 'none'
        option network 'lan'

config wifi-iface 'guest_wlan0'
        option device 'wlan0'
        option mode 'ap'
        option ssid 'PrplOs-guest'
        option encryption 'none'
        option network 'guest'

config wifi-device 'wlan1'
        option type 'mac80211'
        option channel '36'
        option hwmode '11a'
        option path 'platform/soc/a800000.wifi'
        option htmode 'VHT80'
        option disabled '0'

config wifi-iface 'default_wlan1'
        option device 'wlan1'
        option mode 'ap'
        option ssid 'PrplOs'
        option encryption 'none'
        option network 'lan'

config wifi-iface 'guest_wlan1'
        option device 'wlan1'
        option mode 'ap'
        option ssid 'PrplOs-guest'
        option encryption 'none'
        option network 'guest'

